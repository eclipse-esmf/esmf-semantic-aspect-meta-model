@prefix samm: <urn:samm:org.eclipse.samm:meta-model:2.0.0#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix samm-c: <urn:samm:org.eclipse.samm:characteristic:2.0.0#> .
@prefix : <urn:samm:org.eclipse.samm.test:1.0.0#> .

:TestCollectionWithInvalidCharacteristic a samm-c:Collection ;
   samm-c:elementCharacteristic :TestEntity .

:TestEntity a samm:Entity ;
   samm:properties ( :entityProperty ) .

:entityProperty a samm:Property ;
   samm:characteristic samm-c:Text .