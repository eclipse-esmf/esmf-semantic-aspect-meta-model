@prefix : <urn:samm:org.eclipse.samm.test:1.0.0#> .
@prefix samm: <urn:samm:org.eclipse.samm:meta-model:1.0.0#> .
@prefix samm-c: <urn:samm:org.eclipse.samm:characteristic:1.0.0#> .
@prefix unit: <urn:samm:org.eclipse.samm:unit:1.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:TestStructuredValueWithNonMatchingGroups a samm-c:StructuredValue ;
   samm:name "TestStructuredValueWithNonMatchingGroups" ;
   samm:dataType xsd:string ;
   samm-c:deconstructionRule "(prop1x)xx(prop.)" ;
   samm-c:elements ( :prop1 "xxx" :prop2 ) .

:prop1 a samm:Property ;
   samm:name "prop1" ;
   samm:exampleValue "prop1" ;
   samm:characteristic samm-c:Text .

:prop2 a samm:Property ;
   samm:name "prop2" ;
   samm:exampleValue "prop2" ;
   samm:characteristic samm-c:Text .
