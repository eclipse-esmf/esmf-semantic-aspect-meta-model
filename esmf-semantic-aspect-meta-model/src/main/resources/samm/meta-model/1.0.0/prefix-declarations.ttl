@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:1.0.0#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Central definition of namespaces used in SPARQL queries in shapes
samm:prefixDeclarations
   sh:declare [
      sh:prefix "mmm" ;
      sh:namespace "urn:samm:org.eclipse.esmf.samm:meta-meta-model:1.0.0#"^^xsd:anyURI ;
   ] ;
   sh:declare [
      sh:prefix "samm" ;
      sh:namespace "urn:samm:org.eclipse.esmf.samm:meta-model:1.0.0#"^^xsd:anyURI ;
   ] ;
   sh:declare [
      sh:prefix "samm-c" ;
      sh:namespace "urn:samm:org.eclipse.esmf.samm:characteristic:1.0.0#"^^xsd:anyURI ;
   ] ;
   sh:declare [
      sh:prefix "samm-e" ;
      sh:namespace "urn:samm:org.eclipse.esmf.samm:entity:1.0.0#"^^xsd:anyURI ;
   ] ;
   sh:declare [
      sh:prefix "unit" ;
      sh:namespace "urn:samm:org.eclipse.esmf.samm:unit:1.0.0#"^^xsd:anyURI ;
   ] ;
   sh:declare [
      sh:prefix "rdf" ;
      sh:namespace "http://www.w3.org/1999/02/22-rdf-syntax-ns#"^^xsd:anyURI ;
   ] ;
   sh:declare [
      sh:prefix "rdfs" ;
      sh:namespace "http://www.w3.org/2000/01/rdf-schema#"^^xsd:anyURI ;
   ] ;
   sh:declare [
      sh:prefix "sh" ;
      sh:namespace "http://www.w3.org/ns/shacl#"^^xsd:anyURI ;
   ] ;
   sh:declare [
      sh:prefix "dash" ;
      sh:namespace "http://datashapes.org/dash#"^^xsd:anyURI ;
   ] ;
   sh:declare [
      sh:prefix "xsd" ;
      sh:namespace "http://www.w3.org/2001/XMLSchema#"^^xsd:anyURI ;
   ] ;
.

samm:jsValidations
   a sh:JSLibrary ;
   sh:jsLibraryURL "samm://characteristic/1.0.0/characteristic-validations.js"^^xsd:anyURI ;
   sh:jsLibrary samm:languageRegistry .

samm:languageRegistry
   a sh:JSLibrary ;
   sh:jsLibraryURL "samm://scripts/language-registry.js"^^xsd:anyURI .
